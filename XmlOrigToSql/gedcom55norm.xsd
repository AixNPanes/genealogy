<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="gedcom55">
		<xs:annotation>
			<xs:documentation>
				 GEDCOM 6.0 DTD 
	     		 30 December, 2002
    
			     Sample minimal GEDCOM 6.0 XML file:
				(? xml version="1.0" encoding="UTF-8" ?)
				(!DOCTYPE GEDCOM SYSTEM "GEDCOM55.dtd")
				(GEDCOM55)
				( HeaderRec)
				(FileCreation Date="")(/FileCreation)
				(Submitter)
				(Link Target="" Ref="")(/Link)
				(/Submitter)
				(/HeaderRec)
				(IndividualRec Id="testID")
				(/IndividualRec)
				(/GEDCOM55)
			     (Remove extra spaces from xml and HeaderRec tags,
			     added here to allow display in a browser.)

			     GEDCOM 6.0 spec is available at http://www.familysearch.org/GEDCOM/GedXML60.pdf
 
				 GEDCOM55 root 

				<!--
				0 <<HEADER>>															{1:1}
				0 <<SUBMISSION_RECORD>>													{0:1}
				0 <<RECORD>>															{1:M}
				0 TRLR																	{1:1}
				 --> 

				<!--
				RECORD:= 
				[ 
				n <<FAM_RECORD>>														{1:1} 
				| 
				n <<INDIVIDUAL_RECORD>>													{1:1} 
				| 
				n <<MULTIMEDIA_RECORD>>													{1:M} 
				| 
				n <<NOTE_RECORD>>														{1:1} 
				| 
				n <<REPOSITORY_RECORD>>													{1:1} 
				| 
				n <<SOURCE_RECORD>>														{1:1} 
				|
				n <<SUBMITTER_RECORD>>													{1:1} 
				]
				 --> 
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HEADRecord"/>
				<xs:element ref="SUBNRecord" minOccurs="0"/>
				<xs:element ref="INDIRecord" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="FAMRecord" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="OBJERecord" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="NOTERecord" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="REPORecord" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SOURRecord" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SUBMRecord" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TRLRRecord"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="HEADRecord">
		<xs:annotation>
			<xs:documentation>
				<!--
				HEADER:=
				n HEAD																	{1:1}
					+1 SOUR <APPROVED_SYSTEM_ID>										{1:1}
						+2 VERS <VERSION_NUMBER>										{0:1} 
						+2 NAME <NAME_OF_PRODUCT>										{0:1} 
						+2 CORP <NAME_OF_BUSINESS>										{0:1} 
							+3 <<ADDRESS_STRUCTURE>>									{0:1} 
						+2 DATA <NAME_OF_SOURCE_DATA>									{0:1} 
							+3 DATE <PUBLICATION_DATE>									{0:1} 
							+3 COPR <COPYRIGHT_SOURCE_DATA>								{0:1} 
					+1 DEST <RECEIVING_SYSTEM_NAME>										{0:1*} 
					+1 DATE <TRANSMISSION_DATE>											{0:1} 
						+2 TIME <TIME_VALUE>											{0:1} 
					+1 SUBM @XREF:SUBM@													{1:1} 
					+1 SUBN @XREF:SUBN@													{0:1} 
					+1 FILE <FILE_NAME>													{0:1} 
					+1 COPR <COPYRIGHT_GEDCOM_FILE>										{0:1} 
					+1 GEDC																{1:1} 
						+2 VERS <VERSION_NUMBER>										{1:1} 
						+2 FORM <GEDCOM_FORM>											{1:1} 
					+1 CHAR <CHARACTER_SET>												{1:1} 
						+2 VERS <VERSION_NUMBER>										{0:1} 
					+1 LANG <LANGUAGE_OF_TEXT>											{0:1} 
					+1 PLAC																{0:1} 
						+2 FORM <PLACE_HIERARCHY>										{1:1} 
					+1 NOTE <GEDCOM_CONTENT_DESCRIPTION>								{0:1} 
						+2 [CONT|CONC] <GEDCOM_CONTENT_DESCRIPTION>						{0:M}
				 --> 
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SOUR" minOccurs="0">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="valuestruct">
								<xs:sequence>
									<xs:element name="VERS" type="valueline" minOccurs="0"/>
									<xs:element name="NAME" type="valueline" minOccurs="0"/>
									<xs:element name="CORP" type="AddressStructureType" minOccurs="0"/>
									<xs:element name="DATA" minOccurs="0">
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="valuestruct">
													<xs:sequence>
														<xs:element name="DATE" type="valueline" minOccurs="0"/>
														<xs:element name="COPR" type="valueline" minOccurs="0"/>
													</xs:sequence>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="DEST" type="valueline" minOccurs="0"/>
				<xs:element name="DATE" minOccurs="0">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="valuestruct">
								<xs:sequence>
									<xs:element name="TIME" type="valueline" minOccurs="0"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="CHAR">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="valuestruct">
								<xs:sequence>
									<xs:element name="VERS" type="valueline" minOccurs="0"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="FILE" type="valueline" minOccurs="0"/>
				<xs:element name="SUBM" type="valueline"/>
				<xs:element name="SUBN" type="valueline" minOccurs="0"/>
				<xs:element name="COPR" type="valueline" minOccurs="0"/>
				<xs:element name="GEDC">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="struct">
								<xs:sequence>
									<xs:element name="VERS" type="valueline"/>
									<xs:element name="FORM" type="valueline"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="LANG" type="valueline" minOccurs="0"/>
				<xs:element name="PLAC" minOccurs="0">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="struct">
								<xs:sequence>
									<xs:element name="FORM" type="valueline"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="NOTE" minOccurs="0">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="valuestruct">
								<xs:sequence>
									<xs:element name="CONT" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
									<xs:element name="CONC" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="SUBNRecord">
		<xs:annotation>
			<xs:documentation>
				<!--
				SUBMISSION_RECORD:=

				n @XREF:SUBN@ SUBN														{1:1] 
					+1 SUBM @XREF:SUBM@													{0:1} 
					+1 FAMF <NAME_OF_FAMILY_FILE>										{0:1} 
					+1 TEMP <TEMPLE_CODE>												{0:1} 
					+1 ANCE <GENERATIONS_OF_ANCESTORS>									{0:1} 
					+1 DESC <GENERATIONS_OF_DESCENDANTS>								{0:1} 
					+1 ORDI <ORDINANCE_PROCESS_FLAG>									{0:1} 
					+1 RIN <AUTOMATED_RECORD_ID>										{0:1}
				 --> 
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="idstruct">
					<xs:sequence>
						<xs:element name="SUBM" type="valueline" minOccurs="0"/>
						<xs:element name="FAMF" type="valueline" minOccurs="0"/>
						<xs:element name="TEMP" type="valueline" minOccurs="0"/>
						<xs:element name="ANCE" type="valueline" minOccurs="0"/>
						<xs:element name="DESC" type="valueline" minOccurs="0"/>
						<xs:element name="ORID" type="valueline" minOccurs="0"/>
						<xs:element name="RIN" type="valueline" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="OBJERecord">
		<xs:annotation>
			<xs:documentation>
				<!--
				MULTIMEDIA_RECORD:=
				n @XREF:OBJE@ OBJE														{1:1} 
					+1 FORM <MULTIMEDIA_FORMAT>											{1:1} 
					+1 TITL <DESCRIPTIVE_TITLE>											{0:1} 
					+1 <<NOTE_STRUCTURE>>												{0:M} 
					+1 BLOB																{1:1} 
						+2 CONT <ENCODED_MULTIMEDIA_LINE>								{1:M} 
					+1 OBJE @<XREF:OBJE>@     /* chain to continued object */			{0:1} 
					+1 REFN <USER_REFERENCE_NUMBER>										{0:M}
						+2 TYPE <USER_REFERENCE_TYPE>									{0:1} 
					+1 RIN <AUTOMATED_RECORD_ID>										{0:1} 
					+1 <<CHANGE_DATE>>													{0:1}
				 --> 
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="idstruct">
					<xs:sequence>
						<xs:element name="FORM" type="valueline"/>
						<xs:element name="TITL" type="valueline" minOccurs="0"/>
						<xs:element name="NOTE" minOccurs="0"/>
						<xs:element name="BLOB" minOccurs="0">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="valuestruct">
										<xs:sequence>
											<xs:element name="CONT" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="OBJE" type="valueline" minOccurs="0"/>
						<xs:element name="REFN" minOccurs="0">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="valuestruct">
										<xs:sequence>
											<xs:element name="TYPE" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="RIN" type="valueline" minOccurs="0"/>
						<xs:element name="CHAN" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="NOTERecord">
		<xs:annotation>
			<xs:documentation>
				<!--
				NOTE_RECORD:=
				n @<XREF:NOTE>@ NOTE <SUBMITTER_TEXT>									{1:1} 
					+1 [ CONC | CONT] <SUBMITTER_TEXT>									{0:M} 
					+1 <<SOURCE_CITATION>>												{0:M} 
					+1 REFN <USER_REFERENCE_NUMBER>										{0:M}
						+2 TYPE <USER_REFERENCE_TYPE>									{0:1} 
					+1 RIN <AUTOMATED_RECORD_ID>										{0:1} 
					+1 <<CHANGE_DATE>>													{0:1}
				 --> 
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="idstruct">
					<xs:sequence>
						<xs:element name="CONT" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="CONC" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="SOUR" minOccurs="0"/>
						<xs:element name="REFN" minOccurs="0">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="valuestruct">
										<xs:sequence>
											<xs:element name="TYPE" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="RIN" type="valueline" minOccurs="0"/>
						<xs:element name="CHAN" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="SOURRecord">
		<xs:annotation>
			<xs:documentation>
				<!--
				SOURCE_RECORD:=
				n @<XREF:SOUR>@ SOUR													{1:1} 
					+1 DATA																{0:1} 
						+2 EVEN <EVENTS_RECORDED>										{0:M} 
							+3 DATE <DATE_PERIOD>										{0:1} 
							+3 PLAC <SOURCE_JURISDICTION_PLACE>							{0:1} 
						+2 AGNC <RESPONSIBLE_AGENCY>									{0:1} 
						+2 <<NOTE_STRUCTURE>>											{0:M} 
					+1 AUTH <SOURCE_ORIGINATOR>											{0:1} 
						+2 [CONT|CONC] <SOURCE_ORIGINATOR>								{0:M} 
					+1 TITL <SOURCE_DESCRIPTIVE_TITLE>									{0:1} 
						+2 [CONT|CONC] <SOURCE_DESCRIPTIVE_TITLE>						{0:M} 
					+1 ABBR <SOURCE_FILED_BY_ENTRY>										{0:1} 
					+1 PUBL <SOURCE_PUBLICATION_FACTS>									{0:1} 
						+2 [CONT|CONC] <SOURCE_PUBLICATION_FACTS>						{0:M} 
					+1 TEXT <TEXT_FROM_SOURCE>											{0:1} 
						+2 [CONT|CONC] <TEXT_FROM_SOURCE>								{0:M} 
					+1 <<SOURCE_REPOSITORY_CITATION>>									{0:1} 
					+1 <<MULTIMEDIA_LINK>>												{0:M}
					+1 <<NOTE_STRUCTURE>>												{0:M} 
					+1 REFN <USER_REFERENCE_NUMBER>										{0:M}
						+2 TYPE <USER_REFERENCE_TYPE>									{0:1} 
					+1 RIN <AUTOMATED_RECORD_ID>										{0:1} 
					+1 <<CHANGE_DATE>>													{0:1}
				 --> 
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="idstruct">
					<xs:sequence>
						<xs:element name="DATA" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="valuestruct">
										<xs:sequence>
											<xs:element name="EVEN" minOccurs="0" maxOccurs="unbounded">
												<xs:complexType>
													<xs:complexContent>
														<xs:extension base="valuestruct">
															<xs:sequence>
																<xs:element name="DATE" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
																<xs:element name="PLAC" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
															</xs:sequence>
														</xs:extension>
													</xs:complexContent>
												</xs:complexType>
											</xs:element>
											<xs:element name="AGNC" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
											<xs:group ref="NoteStructureGroup" minOccurs="0"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="AUTH" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="valuestruct">
										<xs:sequence>
											<xs:element name="CONT" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
											<xs:element name="CONC" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="TITL" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="valuestruct">
										<xs:sequence>
											<xs:element name="CONT" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
											<xs:element name="CONC" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="ABBR" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="PUBL" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="valuestruct">
										<xs:sequence>
											<xs:element name="CONT" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
											<xs:element name="CONC" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="TEXT" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="valuestruct">
										<xs:sequence>
											<xs:element name="CONT" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
											<xs:element name="CONC" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="SOUR" minOccurs="0"/>
						<xs:element name="OBJE" minOccurs="0"/>
						<xs:element name="NOTE" minOccurs="0"/>
						<xs:element name="REFN" minOccurs="0">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="valuestruct">
										<xs:sequence>
											<xs:element name="TYPE" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="RIN" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="CHAN" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="REPORecord">
		<xs:annotation>
			<xs:documentation>
				<!--
				REPOSITORY_RECORD:=
				n @<XREF:REPO>@ REPO													{1:1} 
					+1 NAME <NAME_OF_REPOSITORY> 										{0:1} 
					+1 <<ADDRESS_STRUCTURE>>											{0:1} 
					+1 <<NOTE_STRUCTURE>>												{0:M} 
					+1 REFN <USER_REFERENCE_NUMBER>										{0:M}
						+2 TYPE <USER_REFERENCE_TYPE>									{0:1} 
					+1 RIN <AUTOMATED_RECORD_ID>										{0:1} 
					+1 <<CHANGE_DATE>>													{0:1}
				 --> 
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="idstruct">
					<xs:sequence>
						<xs:element name="CONT" type="valueline" minOccurs="0"/>
						<xs:group ref="AddressStructureGroup" minOccurs="0"/>
						<xs:element name="NOTE" minOccurs="0"/>
						<xs:element name="REFN" minOccurs="0">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="valuestruct">
										<xs:sequence>
											<xs:element name="TYPE" type="valueline" minOccurs="0" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="RIN" type="valueline" minOccurs="0"/>
						<xs:element name="CHAN" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="SUBMRecord">
		<xs:annotation>
			<xs:documentation>
				<!--
				SUBMITTER_RECORD:=

				n @<XREF:SUBM>@ SUBM													{1:1} 
					+1 NAME <SUBMITTER_NAME>											{1:1} 
					+1 <<ADDRESS_STRUCTURE>>											{0:1} 
					+1 <<MULTIMEDIA_LINK>>												{0:M} 
					+1 LANG <LANGUAGE_PREFERENCE>										{0:3} 
					+1 RFN <SUBMITTER_REGISTERED_RFN>									{0:1} 
					+1 RIN <AUTOMATED_RECORD_ID>										{0:1} 
					+1 <<CHANGE_DATE>>													{0:1}
				 --> 
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="idstruct">
					<xs:sequence>
						<xs:element name="NAME" type="valueline" minOccurs="0"/>
						<xs:group ref="AddressStructureGroup" minOccurs="0"/>
						<xs:element name="EMAIL" type="valueline" minOccurs="0"/>
						<xs:element name="OBJE" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="LANG" type="line" minOccurs="0" maxOccurs="3"/>
						<xs:element name="RFN" type="line" minOccurs="0"/>
						<xs:element name="RIN" type="line" minOccurs="0"/>
						<xs:element name="CHAN" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="INDIRecord">
		<xs:annotation>
			<xs:documentation>
				<!--
				INDIVIDUAL_RECORD:=

				n @XREF:INDI@ INDI														{1:1} 
					+1 RESN <RESTRICTION_NOTICE>										{0:1} 
