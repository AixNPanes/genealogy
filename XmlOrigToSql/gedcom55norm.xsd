														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="QUAY" type="valueline" minOccurs="0"/>
								<xs:element name="OBJE" minOccurs="0" maxOccurs="unbounded"/>
								<xs:element name="CONC" type="valueline"/>
								<xs:element name="CONT" type="valueline"/>
								<xs:element name="TEXT" minOccurs="0" maxOccurs="unbounded">
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="valuestruct">
												<xs:sequence>
